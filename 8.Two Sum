class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int,int>mp;
        int n=nums.size();
        for(int i=0;i<n;i++)
        mp[nums[i]]=i;
       
       vector<int>ans;
        for(int i=0;i<n;i++)
        {
            int x=nums[i];
            int y=(target-x);
            if(mp.find(y)!=mp.end() && mp[y]!=i)
            {
                ans.push_back(i);
                ans.push_back(mp[y]);
                break;
            }
        }
        return ans;
    }
};
