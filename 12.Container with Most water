class Solution {
public:
    int maxArea(vector<int>& height) {
        int n=height.size();
        int i=0,j=n-1;int maxi=0;
        while(i<j)
        {
         int breadth=min(height[i],height[j]);
         int length=j-i;
         int area=length*breadth;
         maxi=max(maxi,area);

         if(height[i]<height[j])i++;
         else j--;
        }
        return maxi;
    }
};
