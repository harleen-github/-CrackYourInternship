class Solution {
public:
    int maxScore(vector<int>&nums, int k) {
        int n=nums.size();
        int lsum=0;
        for(int i=0;i<k;i++)lsum+=nums[i];

        int maxi=lsum;
        int rsum=0;
        int rind=n-1;
        for(int i=k-1;i>=0;i--)
        {
            rsum+=nums[rind];
            rind--;
            lsum-=nums[i];
            maxi=max(maxi,rsum+lsum);
        }
        return maxi;
    }
};
