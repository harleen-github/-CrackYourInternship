class Solution {
public:
    vector<int> nextGreaterElement(vector<int>& nums1, vector<int>& nums2) {
        unordered_map<int, int> nge;
        stack<int> st;

        // Traverse nums2 and populate the next greater element map
        for (int num : nums2) {
            while (!st.empty() && st.top() < num) {
                nge[st.top()] = num;
                st.pop();
            }
            st.push(num);
        }

        // For elements that do not have a next greater element, set it to -1
        while (!st.empty()) {
            nge[st.top()] = -1;
            st.pop();
        }

        // Prepare the result based on the elements of nums1
        vector<int> result;
        for (int num : nums1) {
            result.push_back(nge[num]);
        }

        return result;
    }
};
