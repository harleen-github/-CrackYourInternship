class Solution {
public:
    vector<int> findDuplicates(vector<int>& nums) {
        int n=nums.size();
        vector<int>result;
        for(int i=0;i<n;i++)
        {
             int ind=abs(nums[i]);
             if(nums[ind-1]<0){result.push_back(abs(ind));continue;}
             nums[ind-1]=-1*nums[ind-1];
        }
        return result;
    }
};
