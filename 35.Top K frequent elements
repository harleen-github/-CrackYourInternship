class Solution {
public:
    vector<int> topKFrequent(vector<int>& nums, int k) {
        priority_queue<pair<int,int>>pq;

        unordered_map<int,int>mp;
        int n=nums.size();

        for(int i=0;i<n;i++)
        mp[nums[i]]++;

        for(auto t:mp)
        {
            pq.push({t.second,t.first});
        }
        vector<int>ans;
        while(k--)
        {
            ans.push_back(pq.top().second);
            pq.pop();
        }
        return ans;
    }
};
